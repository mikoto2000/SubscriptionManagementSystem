<p style="color: green"><%= notice %></p>

<h1><%= CommissionMaster.model_name.human %></h1>

<%= render @commission_master %>

<div>
  <%= link_to t("view.show.move_to_update", model: CommissionMaster.model_name.human), edit_commission_master_path(@commission_master) %> |
  <%= link_to t("view.show.move_to_index", model: CommissionMaster.model_name.human.pluralize), commission_masters_path %>

  <%= button_to t("helpers.submit.delete"),
                @commission_master,
                method: :delete,
                class: %w[btn btn-primary],
                data: { turbo_confirm: t("helpers.dialog.delete", id: @commission_master.id) } %>
</div>
