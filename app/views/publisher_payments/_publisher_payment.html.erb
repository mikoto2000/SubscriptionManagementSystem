<div id="<%= dom_id publisher_payment %>">
  <table class="table table-hover">
    <tr>
      <td>
        <strong>Id:</strong>
      </td>
      <td>
        <%= publisher_payment.id %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>
          <%= PublisherPayment.human_attribute_name(:month_for_payment) %>
        </strong>
      </td>
      <td>
        <%= publisher_payment.month_for_payment && l(publisher_payment.month_for_payment) %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>
          <%= PublisherPayment.human_attribute_name(:payment_date) %>
        </strong>
      </td>
      <td>
        <%= publisher_payment.payment_date && l(publisher_payment.payment_date) %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>
          <%= PublisherPayment.human_attribute_name(:payment_status_id) %>
        </strong>
      </td>
      <td>
        <%= publisher_payment.payment_status.name %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>
          <%= PublisherPayment.human_attribute_name(:publisher_id) %>
        </strong>
      </td>
      <td>
        <%= publisher_payment.publisher.name %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>
          <%= PublisherPayment.human_attribute_name(:created_at) %>
        </strong>
      </td>
      <td>
        <%= publisher_payment.created_at && l(publisher_payment.created_at) %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>
          <%= PublisherPayment.human_attribute_name(:updated_at) %>
        </strong>
      </td>
      <td>
        <%= publisher_payment.updated_at && l(publisher_payment.updated_at) %>
      </td>
    </tr>
  </table>
</div>
