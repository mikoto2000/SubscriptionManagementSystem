<p style="color: green"><%= notice %></p>

<h1><%= PublisherPayment.model_name.human %></h1>

<%= render @publisher_payment %>

<div>
  <%= link_to t("view.show.move_to_update", model: PublisherPayment.model_name.human), edit_publisher_payment_path(@publisher_payment) %> |
  <%= link_to t("view.show.move_to_index", model: PublisherPayment.model_name.human.pluralize), publisher_payments_path %>

  <%= button_to t("helpers.submit.delete"),
                @publisher_payment,
                method: :delete,
                class: %w[btn btn-primary],
                data: { turbo_confirm: t("helpers.dialog.delete", id: @publisher_payment.id) } %>
</div>
