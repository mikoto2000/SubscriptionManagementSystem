<div id="<%= dom_id plan_status %>">
  <table class="table table-hover">
    <tr>
      <td>
        <strong>Id:</strong>
      </td>
      <td>
        <%= plan_status.id %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>
          <%= PlanStatus.human_attribute_name(:name) %>
        </strong>
      </td>
      <td>
        <%= plan_status.name %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>
          <%= PlanStatus.human_attribute_name(:created_at) %>
        </strong>
      </td>
      <td>
        <%= plan_status.created_at && l(plan_status.created_at) %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>
          <%= PlanStatus.human_attribute_name(:updated_at) %>
        </strong>
      </td>
      <td>
        <%= plan_status.updated_at && l(plan_status.updated_at) %>
      </td>
    </tr>
  </table>
</div>
